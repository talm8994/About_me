<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context="ctec.about_me.car"
    android:background="@drawable/honda_crv">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I have a green Honda CRV 2007. Like driving with the sunroof open and to drive around with my friends. And hang out with them while jamming out to music  "
        android:id="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:background="#464646" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button3"
        android:background="@drawable/arrow_right"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="314dp" />

</RelativeLayout>
